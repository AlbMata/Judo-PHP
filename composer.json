{
    "name": "judopay/judopay-sdk",
    "type": "library",
    "description": "SDK for the Judopay API",
    "keywords": ["payment"],
    "homepage": "https://www.judopay.com/docs",
    "license": "MIT",
    "authors": [
        {
            "name": "Judopay",
            "email": "developersupport@judopay.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
    ],
    "require": {
        "php": ">=5.3.3",
        "guzzle/guzzle": ">=3.7.0,<=3.9.9",
        "monolog/monolog": "~1.10",
        "pimple/pimple": "~2.1"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "bossa/phpspec2-expect": "~1.0",
        "squizlabs/php_codesniffer": "^2.0",
        "phpunit/phpunit": "^5.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Judopay\\":"src/Judopay"
        },
        "files": [
            "src/Judopay.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}